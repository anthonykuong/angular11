{
  "compileOnSave": false,
  "compilerOptions": {
    "baseUrl": ".",
    "outDir": "./dist/out-tsc",
    "sourceMap": true,
    "declaration": false,
    "downlevelIteration": true,
    "experimentalDecorators": true,
    "module": "es2020",
    "moduleResolution": "node",
    "importHelpers": true,
    "target": "es2015",
    "lib": ["es2018", "dom"],
    "paths": {
      "@frontend-sessions/ui": ["libs/ui/src/index.ts"],
      "@frontend-sessions/profile-update": ["libs/profile-update/src/index.ts"],
      "@frontend-sessions/models": ["libs/models/src/index.ts"],
      "@frontend-sessions/abstractions": ["libs/abstractions/src/index.ts"],
      "@frontend-sessions/test-lib-pub": ["libs/test-lib-pub/src/index.ts"]
    },
    "rootDir": "."
  },
  "angularCompilerOptions": {
    "fullTemplateTypeCheck": true,
    "strictInjectionParameters": true
  }
}

